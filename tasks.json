{
  "version": "2.0.0",
  "tasks": [
{
      "label": "Publish for Linux (Self-Contained)",
      "command": "dotnet",
      "type": "process",
      "args": [
        "publish",
        "${workspaceFolder}/API_NetworkTools/API_NetworkTools.csproj",
        "--configuration",
        "Release",
        "--runtime",
        "linux-x64",
        "--self-contained",
        "true",
        "--output",
        "${workspaceFolder}/API_NetworkTools/bin/publish/linux-x64-selfcontained"
      ],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "reveal": "always",
        "panel": "new"
      }
    }
  ]
}
